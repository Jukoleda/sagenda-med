<div class="container">
    <div class="row">
        <form action="/insertar_contacto" method="post" class="col s5 offset-s5">
            <div class="row">
                <div class="input-field col s6">
                    <input type="text" name="name" id="contact_name" class="validate">
                    <label for="contact_name">Nombre del contacto</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s6">
                    <input type="text" name="detail" id="contact_detail" class="validate">
                    <label for="contact_detail">Detalle</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s6">
                    <input type="text" name="number" id="contact_number" class="validate">
                    <label for="contact_number">Numero telefonico</label>
                </div>
            </div>
            <div class="row">
                <div class="col s12">
                    <button class="btn waves-effect waves-light" type="submit">Agendar</button>
                </div>
            </div>
        </form>
    </div>
</div>